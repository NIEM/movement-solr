    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="em:AffectedQualifierCodeSimpleType">
        <xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
