          <xs:documentation>The alarm is making an audible sound</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Silent">
        <xs:annotation>
          <xs:documentation>The alarm is silent and not making any sound</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="AlarmAudibleDescriptionCodeType">
